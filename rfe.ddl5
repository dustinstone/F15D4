create or replace trigger "F15D4_RFE_SYSADMIN_APPROVAL"
AFTER
insert or update or delete on "F15D4_RFE"
for each row
begin
INSERT INTO F15D4_RFE_APPROVAL (EMPLOYEE_TYPE, RFE_ID, LAB_ID) VALUES ('Sys Admin', :NEW.rfe_id, :NEW.lab_id);
end;

create or replace trigger "F15D4_RFE_APPROVAL_SA_A"
BEFORE
update on "F15D4_RFE_APPROVAL"
for each row
begin
  IF (:NEW.employee_type = 'Sys Admin') THEN
     :NEW.employee_type := 'Lab Director';
  END IF;
end;

